<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.new_mynews.ui.News.News_Headline"
        android:label="@string/title_home"
        tools:layout="@layout/news_headline" />

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.new_mynews.ui.Search.News_Search"
        android:label="@string/title_dashboard"
        tools:layout="@layout/news_search" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.new_mynews.ui.Setting.News_Setting"
        android:label="@string/title_notifications"
        tools:layout="@layout/news_setting" />
</navigation>